{
  "name": "wc-react-remote",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/wc-react-remote/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rspack:rspack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "web",
        "outputPath": "dist/apps/wc-react-remote",
        "main": "apps/wc-react-remote/src/main.ts",
        "tsConfig": "apps/wc-react-remote/tsconfig.app.json",
        "rspackConfig": "apps/wc-react-remote/rspack.config.ts",
        "assets": [
          "apps/wc-react-remote/src/favicon.ico",
          "apps/wc-react-remote/src/assets"
        ]
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production",
          "optimization": true,
          "sourceMap": false,
          "rspackConfig": "apps/wc-react-remote/rspack.config.prod.ts"
        }
      }
    },
    "serve": {
      "executor": "@nx/rspack:module-federation-dev-server",
      "options": {
        "buildTarget": "wc-react-remote:build:development",
        "port": 4201
      },
      "configurations": {
        "development": {},
        "production": {
          "buildTarget": "wc-react-remote:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "serve-static": {
      "executor": "@nx/rspack:module-federation-static-server",
      "defaultConfiguration": "production",
      "options": {
        "serveTarget": "wc-react-remote:serve"
      },
      "configurations": {
        "development": {
          "serveTarget": "wc-react-remote:serve:development"
        },
        "production": {
          "serveTarget": "wc-react-remote:serve:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/wc-react-remote/jest.config.ts"
      }
    }
  }
}
